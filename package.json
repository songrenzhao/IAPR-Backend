    "@babel/runtime": "^7.8.4",
    "apollo-server-express": "^2.10.1",
    "cors": "^2.8.5",
    "dotenv": "^8.2.0",
    "express": "^4.17.3",
    "graphql": "^14.6.0",
    "husky": "^4.2.3",
    "mongoose": "^5.9.2",
    "nodemon": "^2.0.2"
  },
  "devDependencies": {
